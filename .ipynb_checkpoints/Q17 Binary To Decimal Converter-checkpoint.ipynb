   "source": [
    "b_num = list(input(\"Enter a Binary number: \"))\n",
    "bn = b_num\n",
    "value = 0\n",
    "\n",
    "for i in range(len(b_num)):\n",
    "    digit = b_num.pop()\n",
    "    if digit == '1':\n",
    "        value = value + pow(2, i)\n",
    "print(\"Decimal Representation of {} is {}\".format(str(bn,value))\n"
   ]
