   "source": [
    "text = input(\"Enter text: \")\n",
    "re = \"\"\n",
    "for i in range(len(text)-1,-1,-1):\n",
    "    re = re+text[i]\n",
    "    print(re)\n",
    "if re == text:\n",
    "    print(\"Text {} is Palindrome\".format(text))\n",
    "else:\n",
    "    print(\"Text {} is not Palindrome\".format(text))"
   ]
