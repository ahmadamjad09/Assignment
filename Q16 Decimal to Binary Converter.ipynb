   "source": [
    "n = input(\"Enter a Decimal Number: \")\n",
    "n = int(n)\n",
    "nb = bin(n)\n",
    "nbb = \"\"\n",
    "for i in range(2,len(nb),1):\n",
    "    nbb = nbb + nb[i]\n",
    "print(\"Binary Representation of {} is {}\".format(n,nbb))"
